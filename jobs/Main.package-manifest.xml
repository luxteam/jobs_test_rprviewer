<?xml version="1.0" ?>
<package-manifest name="Main">
    <variable name="Root" value="{CWD}"/>
    <variable name="ResourcesDir" value = "{CWD}/Scripts"/>
    <variable name="ScenesDir" value="{ResPath}"/>
    <variable name="CommonDir" value="{CWD}/../jobs_launcher/common"/>
    <variable name="BaselineDir" value="{CWD}/../Work/Baseline"/>
    <variable name="RenderPath" value="{CWD}/../RprViewer/"/>

    <option name="RenderEngine">
        <configuration name="Hybrid">
            <value>hyb</value>
        </configuration>
    </option>

    <option name="RenderQuality">
        <configuration name="High">
            <value>2</value>
        </configuration>
    </option>
</package-manifest>

